.publicitylist{
  display: flex;
  flex-direction: column;
  
}
.publicitylist-item{
  display: flex;
  flex-direction: column;
  margin-right: 30rpx;
  margin-left: 30rpx;
}
.publicitylist-item-title{
  color: #333;
  font-size: 28rpx;
  margin-top: 30rpx;
}
.publicitylist-item-date{
  color: #939393;
  font-size: 20rpx;
  margin-top: 20rpx;
  margin-bottom: 30rpx;
   
}
.publicitylist-item-describe{
  color: #333;
  font-size: 24rpx;
  margin-bottom: 30rpx;
}

.publicitylist-line{
  width: 100%;
  height: 2rpx;
  background-color: #eaeaea
}