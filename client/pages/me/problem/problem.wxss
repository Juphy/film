.problemlist {
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.problemlist-item {
  display: flex;
  flex-direction: row;
  height: 100rpx;
  align-items: center;
}

.problemlist-title {
  font-size: 24rpx;
  color: #333;
  font-weight: 700;
  flex-grow: 1;
}

.problemList-line {
  width: 100%;
  height: 2rpx;
  background-color: #eaeaea;
}

.problemlist-image {
  width: 10rpx;
  height: 20rpx;
}
