.award-address{
  display: flex;
  flex-direction: column;
  background: #eaeaea;
  height: 100vh;
  font-size: 24rpx;
}

.address-info{
  background: #fff;
  padding: 30rpx;
  display: flex;
  flex-direction: column;
}


.address-info-change-icon{
  width: 9rpx;
  height: 18rpx;
}

.address-info-name-phone{
  display: flex;
  flex-direction: row;
  font-weight: 700;
}

.address-info-address{
  font-weight: 700;
  
}

.address-info-change{
  color: #939393;
  margin-top: 15rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
}

.address-info-change-text{
margin-right: 10rpx;

}
.address-info-name{
  margin-bottom: 15rpx;
  flex-grow: 1;
}


.address-info-submit{
  margin-top: 60rpx;
  background: #56bdac;
  margin-left: 30rpx;
  margin-right: 30rpx;
  color: #fff;
  font-size: 28rpx;

}


.addres-info-empty-icon{
  width: 101rpx;
  height: 138rpx;
  margin-top: 60rpx;
  
}

.address-info-empty{
  display: flex;
  flex-direction: column;
align-items: center;
}