<view class='award'>

  <block wx:for='{{awardList}}'>
    <view class='award-list-item'>
      <text class='award-item-title'>{{item.title}}</text>




<view  class='award-item-state'>

      <text data-type='{{item.awardtype}}' data-state='{{item.state}}' bindtap='{{item.state==true?"onClickAward":""}}' class='award-item-state-{{item.state?"y":"n"}} ' >{{item.state?'领奖':'已领'}}</text>
</view>
    </view>
  </block>

</view>