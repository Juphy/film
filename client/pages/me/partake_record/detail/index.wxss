.partake-record {
  display: flex;
  flex-direction: column;
  background-color: #eaeaea;
}

.partake-record-title {
  font-size: 28rpx;
  color: #333;
  font-weight: 700;
  margin-left: 60rpx;
  margin-right: 60rpx;
  justify-content: center;
  margin-top: 30rpx;
}

.partake-record-item {
  margin-top: 30rpx;
  margin-left: 60rpx;
  margin-right: 60rpx;
  background-color: #fff;
  border-radius: 25rpx;
  padding: 30rpx;
  display: flex;
  flex-direction: column;
}

.partake-record-date-del {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.partake-record-del {
  width: 80rpx;
  height: 80rpx;
}

.partake-record-date {
  font-size: 24rpx;
  flex-grow: 1;
  font-weight: 700;
}

.partake-record-line {
  width: 100%;
  height: 2rpx;
  background-color: #eaeaea;
}

.partake-record-film-name{
    font-size: 24rpx;
  font-weight: 700;
  margin-top: 30rpx;
  margin-bottom: 10rpx;
}

.partake-record-film-address {
  font-size: 24rpx;
  color: #939393;
  margin-bottom: 30rpx;
}

.partake-record-film-demo{
  font-size: 24rpx;
  color: #939393;
  margin-top: 30rpx;
}

.partake-record-images {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}

.partake-record-images-item {
  width: 170rpx;
  height: 170rpx;
  margin-top: 30rpx;
}

.pratake-record-detail{
    width: 100%;
  height: 98rpx;
  background-color: #56bdac;
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
font-size: 28rpx;
}
