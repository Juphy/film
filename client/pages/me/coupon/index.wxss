.award {
  padding-top: 30rpx;
  background: #eaeaea;
  width: 100%;
  height: 100vh;
}

.award-list-item {

  background: #fff;
  display: flex;
  flex-direction: column;
  margin-bottom: 10rpx;
   margin-left: 30rpx;
  margin-right: 30rpx;
}

.award-item-title {
 
  display: flex;
  flex-direction: column;
  height: 144rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}

.award-item-state-y {
  width: 120rpx;
  height: 50rpx;
  border-radius: 6rpx;
  background: #56bdac;
  color: #fff;
  font-size: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-bottom: 30rpx;
}

.award-item-state-n {
  width: 100rpx;
  height: 50rpx;
  border-radius: 6rpx;
  background: #939393;
  color: #fff;
  font-size: 24rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-bottom: 30rpx;
}

.award-item-state {
  display: flex;
  flex: row;
  justify-content: flex-end;
  margin-right: 30rpx;
}

.award-item-icocn-title {
  display: flex;
  flex: row;
  margin-top: 30rpx;
  margin-right: 30rpx;
  margin-left: 30rpx;
}

.award-item-icon {
  width: 90rpx;
  height: 90rpx;
}
